{
  "name": "bulky",
  "description": "Extensible JavaScript file upload widget with support for drag&drop, resumable uploads, previews, restrictions, file processing/encoding, remote providers like Instagram, Dropbox, Google Drive, S3 and more :dog:",
  "version": "3.3.0",
  "license": "MIT",
  "main": "index.mjs",
  "module": "index.mjs",
  "unpkg": "dist/bulky.min.js",
  "style": "dist/bulky.min.css",
  "types": "types/index.d.ts",
  "keywords": [
    "file uploader",
    "drag-drop",
    "progress",
    "preview",
    "resumable uploads",
    "xhr",
    "tus",
    "s3",
    "google drive",
    "dropbox",
    "box",
    "webcam"
  ],
  "homepage": "https://bulky.io",
  "bugs": {
    "url": "https://github.com/transloadit/bulky/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transloadit/bulky.git"
  },
  "dependencies": {
    "@bulky/audio": "workspace:^",
    "@bulky/aws-s3": "workspace:^",
    "@bulky/aws-s3-multipart": "workspace:^",
    "@bulky/box": "workspace:^",
    "@bulky/companion-client": "workspace:^",
    "@bulky/compressor": "workspace:^",
    "@bulky/core": "workspace:^",
    "@bulky/dashboard": "workspace:^",
    "@bulky/drag-drop": "workspace:^",
    "@bulky/drop-target": "workspace:^",
    "@bulky/dropbox": "workspace:^",
    "@bulky/facebook": "workspace:^",
    "@bulky/file-input": "workspace:^",
    "@bulky/form": "workspace:^",
    "@bulky/golden-retriever": "workspace:^",
    "@bulky/google-drive": "workspace:^",
    "@bulky/image-editor": "workspace:^",
    "@bulky/informer": "workspace:^",
    "@bulky/instagram": "workspace:^",
    "@bulky/onedrive": "workspace:^",
    "@bulky/progress-bar": "workspace:^",
    "@bulky/provider-views": "workspace:^",
    "@bulky/redux-dev-tools": "workspace:^",
    "@bulky/remote-sources": "workspace:^",
    "@bulky/screen-capture": "workspace:^",
    "@bulky/status-bar": "workspace:^",
    "@bulky/store-default": "workspace:^",
    "@bulky/store-redux": "workspace:^",
    "@bulky/thumbnail-generator": "workspace:^",
    "@bulky/transloadit": "workspace:^",
    "@bulky/tus": "workspace:^",
    "@bulky/unsplash": "workspace:^",
    "@bulky/url": "workspace:^",
    "@bulky/webcam": "workspace:^",
    "@bulky/xhr-upload": "workspace:^",
    "@bulky/zoom": "workspace:^"
  },
  "devDependencies": {
    "abortcontroller-polyfill": "^1.7.3",
    "core-js": "~3.24.0",
    "md-gum-polyfill": "^1.0.0",
    "regenerator-runtime": "0.13.9",
    "resize-observer-polyfill": "^1.5.1",
    "whatwg-fetch": "^3.6.2"
